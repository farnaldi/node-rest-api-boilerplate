{
  "name": "node-rest-api-boilerplate",
  "version": "0.0.1",
  "engines": {
    "node": ">= 10.15.0"
  },
  "repository": "https://github.com/farnaldi/node-rest-api-boilerplate",
  "description": "Node rest API boilerplate",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "tsc",
    "start": "npm run build && nodemon ./dist/src/index.js",
    "dev": "nodemon --exec ts-node -- ./src/index.ts",
    "lint": "eslint src/**/*.ts --fix",
    "knex:migrate:make": "knex --knexfile knexfile.ts -x js migrate:make",
    "knex:migrate:latest": "knex --knexfile knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback"
  },
  "keywords": [
    "node",
    "express",
    "knex",
    "api",
    "boilerplate"
  ],
  "author": "farnaldi",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/hapi__joi": "^16.0.12",    
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sinon": "^9.0.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "http-status": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.28",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "objection": "^2.1.3"
  },
  "jest": {
    "collectCoverage": true
  }
}
