{
  "name": "node-rest-api-boilerplate",
  "version": "0.0.1",
  "engines": {
    "node": ">= 10.15.0"
  },
  "repository": "https://github.com/farnaldi/node-rest-api-boilerplate",
  "description": "Node rest API boilerplate",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "npm run build && nodemon ./dist/src/index.js",
    "dev": "nodemon --exec ts-node -- ./src/index.ts",
    "lint": "eslint src/**/*.ts --fix",
    "knex:migrate:make": "knex --knexfile knexfile.ts -x js migrate:make",
    "knex:migrate:latest": "knex --knexfile knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback"
  },
  "keywords": [
    "node",
    "express",
    "knex",
    "api",
    "boilerplate"
  ],
  "author": "farnaldi",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/hapi__joi": "^16.0.6",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "http-status": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.2",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "objection": "^1.6.11"
  }
}
